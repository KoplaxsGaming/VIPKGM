instal()
{
mount -o rw,remount /system
mkdir /storage/emulated/0/Android/data/kgm.vip.data
mkdir /storage/emulated/0/Android/data/kgm.vip.data/Global
mkdir /storage/emulated/0/Android/data/kgm.vip.data/Global/bDATA
mkdir /storage/emulated/0/Android/data/kgm.vip.data/Global/bLIB
cp -R /data/data/com.tencent.ig/lib/* /storage/emulated/0/Android/data/kgm.vip.data/Global/bLIB
sleep 1
wget https://raw.githubusercontent.com/nggandenltd/bypasspatch/master/data1s11.dat
wget https://raw.githubusercontent.com/nggandenltd/bypasspatch/master/data2s11.dat
wget https://raw.githubusercontent.com/nggandenltd/bypasspatch/master/data.dat
wget https://raw.githubusercontent.com/nggandenltd/bypasspatch/master/data2.dat
wget https://raw.githubusercontent.com/nggandenltd/bypasspatch/master/data3.dat
wget https://raw.githubusercontent.com/nggandenltd/bypasspatch/master/data4.dat
wget https://raw.githubusercontent.com/nggandenltd/bypasspatch/master/data5.dat
sleep 2
mv data1s11.dat patchfilekgm
mv data2s11.dat patchfilekgm2
sleep 1
mv patchfilekgm /storage/emulated/0/Android/data/kgm.vip.data/Global
mv patchfilekgm2 /storage/emulated/0/Android/data/kgm.vip.data/Global
mv data.dat /storage/emulated/0/Android/data/kgm.vip.data/Global
mv data2.dat /storage/emulated/0/Android/data/kgm.vip.data/Global
mv data3.dat /storage/emulated/0/Android/data/kgm.vip.data/Global
mv data4.dat /storage/emulated/0/Android/data/kgm.vip.data/Global
mv data5.dat /storage/emulated/0/Android/data/kgm.vip.data/Global
sleep 1
echo "Installation Finished"
}
urdata()
{
mv /storage/emulated/0/Android/data/kgm.vip.data/Global/patchfilekgm /storage/emulated/0/Android/data/kgm.vip.data/Global/game_patch_0.16.5.11559.pak
mv /storage/emulated/0/Android/data/kgm.vip.data/Global/patchfilekgm2 /storage/emulated/0/Android/data/kgm.vip.data/Global/game_patch_0.16.5.11560.pak
mv /storage/emulated/0/Android/data/kgm.vip.data/Global/data.dat /storage/emulated/0/Android/data/kgm.vip.data/Global/libtersafe.so
mv /storage/emulated/0/Android/data/kgm.vip.data/Global/data2.dat /storage/emulated/0/Android/data/kgm.vip.data/Global/libhelpshiftlistener.so
mv /storage/emulated/0/Android/data/kgm.vip.data/Global/data3.dat /storage/emulated/0/Android/data/kgm.vip.data/Global/libtprt.so
mv /storage/emulated/0/Android/data/kgm.vip.data/Global/data4.dat /storage/emulated/0/Android/data/kgm.vip.data/Global/libUE4.so
mv /storage/emulated/0/Android/data/kgm.vip.data/Global/data5.dat /storage/emulated/0/Android/data/kgm.vip.data/Global/libzip.so
}
rndata()
{
mv /storage/emulated/0/Android/data/kgm.vip.data/Global/game_patch_0.16.5.11559.pak /storage/emulated/0/Android/data/kgm.vip.data/Global/patchfilekgm
mv /storage/emulated/0/Android/data/kgm.vip.data/Global/game_patch_0.16.5.11560.pak /storage/emulated/0/Android/data/kgm.vip.data/Global/patchfilekgm2
mv /storage/emulated/0/Android/data/kgm.vip.data/Global/libtersafe.so /storage/emulated/0/Android/data/kgm.vip.data/Global/data.dat
mv /storage/emulated/0/Android/data/kgm.vip.data/Global/libhelpshiftlistener.so /storage/emulated/0/Android/data/kgm.vip.data/Global/data2.dat
mv /storage/emulated/0/Android/data/kgm.vip.data/Global/libtprt.so /storage/emulated/0/Android/data/kgm.vip.data/Global/data3.dat
mv /storage/emulated/0/Android/data/kgm.vip.data/Global/libUE4.so /storage/emulated/0/Android/data/kgm.vip.data/Global/data4.dat
mv /storage/emulated/0/Android/data/kgm.vip.data/Global/libzip.so /storage/emulated/0/Android/data/kgm.vip.data/Global/data5.dat
}
bypass()
{
urdata
cp -R /storage/emulated/0/Android/data/kgm.vip.data/Global/game_patch_0.16.5.11560.pak /storage/emulated/0/Android/data/com.tencent.ig/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks
cp -R /storage/emulated/0/Android/data/kgm.vip.data/Global/game_patch_0.16.5.11559.pak /storage/emulated/0/Android/data/com.tencent.ig/files/UE4Game/ShadowTrackerExtra/ShadowTrackerExtra/Saved/Paks
rndata
}
modstart()
{
rm -rf /data/data/com.tencent.ig/lib/libhelpshiftlistener.so
rm -rf /data/data/com.tencent.ig/lib/libtprt.so
rm -rf /data/data/com.tencent.ig/lib/libUE4.so
rm -rf /data/data/com.tencent.ig/lib/libzip.so
rm -rf /data/data/com.tencent.ig/lib/libtersafe.so
sleep 1
urdata
cp -R /storage/emulated/0/Android/data/kgm.vip.data/Global/libtersafe.so /data/data/com.tencent.ig/lib
cp -R /storage/emulated/0/Android/data/kgm.vip.data/Global/libhelpshiftlistener.so /data/data/com.tencent.ig/lib
cp -R /storage/emulated/0/Android/data/kgm.vip.data/Global/libtprt.so /data/data/com.tencent.ig/lib
cp -R /storage/emulated/0/Android/data/kgm.vip.data/Global/libUE4.so /data/data/com.tencent.ig/lib
cp -R /storage/emulated/0/Android/data/kgm.vip.data/Global/libzip.so /data/data/com.tencent.ig/lib
rndata
chmod -R 755 /data/data/com.tencent.ig/lib/libhelpshiftlistener.so
chmod -R 755 /data/data/com.tencent.ig/lib/libtprt.so
chmod -R 755 /data/data/com.tencent.ig/lib/libUE4.so
chmod -R 000 /data/data/com.tencent.ig/lib/libgcloud.so
chmod -R 000 /data/data/com.tencent.ig/lib/libBugly.so
chmod -R 000 /data/data/com.tencent.ig/lib/libzlib.so
}
